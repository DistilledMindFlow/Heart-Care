{
    "builds": [
        {
            "src": "heartcare/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"  // or "python3.9" if supported
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "heartcare/wsgi.py"
        }
    ]
}
